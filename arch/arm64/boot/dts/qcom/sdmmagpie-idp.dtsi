&qupv3_se7_i2c {
	qcom,i2c-touch-active="st,fts";
	status = "ok";

	st_fts@49 {
		compatible = "st,fts";
		reg = <0x49>;
		interrupt-parent = <&tlmm>;
		interrupts = <9 0x2008>;
		vdd-supply = <&pm6150_l10>;
		avdd-supply = <&pm6150l_l7>;
		pinctrl-names = "pmx_ts_active", "pmx_ts_suspend",
					"pmx_ts_release";
		pinctrl-0 = <&ts_active>;
		pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;
		pinctrl-2 = <&ts_release>;
		st,irq-gpio = <&tlmm 9 0x2008>;
		st,reset-gpio = <&tlmm 8 0x00>;
		st,regulator_dvdd = "vdd";
		st,regulator_avdd = "avdd";
		st,x-flip;
		st,y-flip;
		status = "disabled";
	};

	synaptics_dsx@20 {
		compatible = "synaptics,dsx-i2c";
		reg = <0x20>;
		interrupt-parent = <&tlmm>;
		interrupts = <9 0x2008>;
		vdd-supply = <&pm6150_l10>;
		avdd-supply = <&pm6150l_l7>;
		pinctrl-names = "pmx_ts_active","pmx_ts_suspend",
					"pmx_ts_release";
		pinctrl-0 = <&ts_active>;
		pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;
		pinctrl-2 = <&ts_release>;
		synaptics,pwr-reg-name = "avdd";
		synaptics,bus-reg-name = "vdd";
		synaptics,ub-i2c-addr = <0x20>;
		synaptics,max-y-for-2d = <2159>;
		synaptics,irq-gpio = <&tlmm 9 0x2008>;
		synaptics,reset-gpio = <&tlmm 8 0x0>;
		synaptics,irq-on-state = <0>;
		synaptics,power-delay-ms = <200>;
		synaptics,reset-delay-ms = <200>;
		synaptics,reset-on-state = <0>;
		synaptics,reset-active-ms = <20>;
		status = "disabled";
	};
	goodix-gt738x@5d {
		compatible = "goodix,gt738x";
		reg = <0x5d>;
		goodix,reset-gpio = <&tlmm 8 0x0>;
		goodix,irq-gpio = <&tlmm 9 0x2008>;
		goodix,irq-flags = <2>; /* 1:trigger rising, 2:trigger falling;*/
		goodix,panel-max-x = <2560>;
		goodix,panel-max-y = <1600>;
		goodix,panel-max-w = <2368>;
		goodix,avdd-name ="avdd";
		goodix,avdd-en-gpio =<&pm6150l_gpios 7 0>;
		avdd-supply = <&pm6150l_l7>;
		pinctrl-names = "pmx_ts_active", "pmx_ts_suspend", "pmx_ts_release","avddengpio";
		pinctrl-0 = <&ts_active &gpio7_avdden_out_default>;
		pinctrl-1 = <&ts_int_suspend &ts_reset_suspend>;
		pinctrl-2 = <&ts_release>;
	};
};

&pm6150_qg {
	qcom,battery-data = <&mtp_batterydata>;
	qcom,qg-iterm-ma = <450>;
	qcom,hold-soc-while-full;
	qcom,linearize-soc;
	qcom,cl-feedback-on;
};

&pm6150_charger {
	io-channels = <&pm6150_vadc ADC_USB_IN_V_16>,
		      <&pm6150_vadc ADC_USB_IN_I>,
		      <&pm6150_vadc ADC_CHG_TEMP>,
		      <&pm6150_vadc ADC_DIE_TEMP>,
		      <&pm6150l_vadc ADC_AMUX_THM1_PU2>,
		      <&pm6150_vadc ADC_SBUx>,
		      <&pm6150_vadc ADC_VPH_PWR>;
	io-channel-names = "usb_in_voltage",
			   "usb_in_current",
			   "chg_temp",
			   "die_temp",
			   "conn_temp",
			   "sbux_res",
			   "vph_voltage";
	qcom,battery-data = <&mtp_batterydata>;
	qcom,auto-recharge-vbat-mv = <4330>;
	qcom,sw-jeita-enable;
	qcom,sec-charger-config = <1>;
	qcom,thermal-mitigation = <4200000 3500000 3000000
			2500000 2000000 1500000 1000000 500000>;
	dpdm-supply = <&qusb_phy0>;
	qcom,charger-temp-max = <800>;
	qcom,smb-temp-max = <800>;
	qcom,chg-term-src = <1>;
	qcom,chg-term-current-ma = <(-400)>;
	qcom,usb-icl-ua = <2000000>;
	qcom,float-option = <2>;
	qcom,boost_en=<&pm6150l_gpios 11 0>;
	pinctrl-names = "default";
	pinctrl-0 = <&pogo_gpio>;
	qcom,pogo_det-gpio=<&tlmm 94 0>;
	qcom,typec_det-gpio=<&tlmm 66 0>;
	qcom,pogo_en-gpio=<&tlmm 65 0>;
	qcom,typec_en-gpio=<&tlmm 74 0>;
};

&pm6150_gpios {
	smb_stat {
		smb_stat_default: smb_stat_default {
			pins = "gpio3";
			function = "normal";
			input-enable;
			bias-pull-up;
			qcom,pull-up-strength = <PMIC_GPIO_PULL_UP_30>;
			power-source = <0>;
		};
	};

	hwid {
		hwid_gpio6: hwid_gpio6 {
			pins = "gpio6";
			function = "normal";
			input-enable;
			bias-no-pull;
			power-source = <0>;
		};

		hwid_gpio7: hwid_gpio7 {
			pins = "gpio7";
			function = "normal";
			input-enable;
			bias-no-pull;
			power-source = <0>;
		};
	};
};

&qupv3_se9_i2c {
	status = "ok";
	#include "smb1390.dtsi"
	#include "smb1355.dtsi"
	bq27541@55 {
		compatible = "ti,bq27541";
		reg = <0x55>;
	};
	redriver:redriver@19 {
                compatible = "onnn,redriver";
                reg = <0x19>;
                extcon = <&pm6150_pdphy>, <&pm6150_pdphy>;
		eq = /bits/ 8 <
				/* Parameters for USB */
				0x7 0x4 0x4 0x7
				/* Parameters for DP */
				0x6 0x4 0x4 0x6>;
		flat-gain = /bits/ 8 <
				/* Parameters for USB */
				0x3 0x1 0x1 0x3
				/* Parameters for DP */
				0x0 0x3 0x3 0x0>;
		output-comp = /bits/ 8 <
				/* Parameters for USB */
				0x3 0x3 0x3 0x3
				/* Parameters for DP */
				0x3 0x3 0x3 0x3>;
		loss-match = /bits/ 8 <
				/* Parameters for USB */
				0x1 0x3 0x3 0x1
				/* Parameters for DP */
				0x2 0x2 0x2 0x2>;
        };
	keyboard:  keyboard@61 {
			      compatible = "hid-over-i2c";
			      reg = <0x61>;
		          vdd-supply = <&pm6150_l16>;
		          post-power-on-delay-ms = <100>;
		          interrupt-parent = <&tlmm>;
		          interrupts = <5 0x2002>;
		          hid-descr-addr = <0x0001>;
		         };

	fan49103:fan49103@70 {
		compatible = "onsemi,fan49103";
		reg = <0x70>;
		onsemi,pwrgood-gpio = <&pm6150l_gpios 1 0>;
		interrupt-parent = <&spmi_bus>;
		pinctrl-names = "default";
		pinctrl-0 = <&fan_irq_pins_default>;
	};
};

&smb1355 {
	/delete-property/ interrupts;
	interrupts = <0x0 0xc2 0x0 IRQ_TYPE_LEVEL_LOW>;
	status = "disabled";
};

&smb1355_charger {
	status = "disabled";
};

&smb1390 {
	/delete-property/ interrupts;
	interrupts = <0x0 0xc2 0x0 IRQ_TYPE_LEVEL_LOW>;
	pinctrl-names = "default";
	pinctrl-0 = <&smb_stat_default>;
	status = "ok";
};

&smb1390_charger {
	compatible = "qcom,smb1390-charger-psy";
	io-channels = <&pm6150l_vadc ADC_AMUX_THM2>;
	io-channel-names = "cp_die_temp";
	status = "ok";
};

&pm6150l_gpios {
	key_vol_up {
		key_vol_up_default: key_vol_up_default {
			pins = "gpio2";
			function = "normal";
			input-enable;
			bias-pull-up;
			power-source = <0>;
		};
	};
	tp_avdden {
		gpio7_avdden_out_default:gpio7_avdden_out_default{
			pins = "gpio7";
			function = "normal";
			power-source = <1>;
			ouput-low;
			input-disable;
			    };
			};
		};

&soc {
	gpio_keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-names = "default";
		pinctrl-0 = <&key_vol_up_default>;

		vol_up {
			label = "volume_up";
			gpios = <&pm6150l_gpios 2 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			linux,can-disable;
			debounce-interval = <15>;
			gpio-key,wakeup;
		};
	};
};

&thermal_zones {
	quiet-therm-step {
		status = "disabled";
	};
};
